(define (problem floortile-2-2-2)
 (:domain floor-tile)
 (:objects tile_1 tile_5 
           tile_2 tile_4 
           tile_3 tile_0 - tile
           robot_1 robot_0 - robot
           color_1 color_0 - color
)
 (:init 
   (robot-at robot_1 tile_4)
   (robot-has robot_1 color_1)
   (robot-at robot_0 tile_2)
   (robot-has robot_0 color_0)
   (available-color color_1)
   (available-color color_0)
   (clear tile_1)
   (clear tile_5)
   (clear tile_3)
   (clear tile_0)
   (up tile_2 tile_1)
   (up tile_4 tile_5)
   (up tile_3 tile_2)
   (up tile_0 tile_4)
   (down tile_1 tile_2)
   (down tile_5 tile_4)
   (down tile_2 tile_3)
   (down tile_4 tile_0)
   (right tile_5 tile_1)
   (right tile_4 tile_2)
   (right tile_0 tile_3)
   (left tile_1 tile_5)
   (left tile_2 tile_4)
   (left tile_3 tile_0)
)
 (:goal (and
    (painted tile_2 color_1)
    (painted tile_4 color_0)
    (painted tile_3 color_0)
    (painted tile_0 color_1)
))
)
